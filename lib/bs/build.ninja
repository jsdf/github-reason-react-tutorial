bs_package_flags = -bs-package-name reason-scripts
src_root_dir = /Users/jfriend/code/github-reason-list
bsc = /usr/local/lib/node_modules/bs-platform/bin/bsc.exe
bsdep = /usr/local/lib/node_modules/bs-platform/bin/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -nostdlib -I '/Users/jfriend/code/github-reason-list/node_modules/bs-platform/lib/ocaml' -bs-super-errors -no-alias-deps -color always
ppx_flags = 
bs_package_includes = -I /Users/jfriend/code/github-reason-list/node_modules/reason-react/lib/ocaml -I /Users/jfriend/code/github-reason-list/node_modules/bs-jest/lib/ocaml -I /Users/jfriend/code/github-reason-list/node_modules/bs-fetch/lib/ocaml -I /Users/jfriend/code/github-reason-list/node_modules/bs-json/lib/ocaml
bs_package_dev_includes = 
refmt = /usr/local/lib/node_modules/bs-platform/bin/refmt3.exe
reason_react_jsx = -ppx '/usr/local/lib/node_modules/bs-platform/bin/reactjs_jsx_ppx_2.exe'
refmt_flags = --print binary
namespace = 
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/app.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/app.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/app.mlast.d : build_deps src/app.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/app.cmj |  $src_root_dir/lib/js/src/app.js src/app.cmi : build_cmj_cmi src/app.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/index.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/index.re
build src/index.mlast.d : build_deps src/index.mlast
build src/index.cmj |  $src_root_dir/lib/js/src/index.js src/index.cmi : build_cmj_cmi src/index.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/app_test.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/app_test.re
build src/app_test.mlast.d : build_deps src/app_test.mlast
build src/app_test.cmj |  $src_root_dir/lib/js/src/app_test.js src/app_test.cmi : build_cmj_cmi src/app_test.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/RepoData.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/RepoData.re
build src/RepoData.mlast.d : build_deps src/RepoData.mlast
build src/RepoData.cmj |  $src_root_dir/lib/js/src/RepoData.js src/RepoData.cmi : build_cmj_cmi src/RepoData.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/RepoItem.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/RepoItem.re
build src/RepoItem.mlast.d : build_deps src/RepoItem.mlast
build src/RepoItem.cmj |  $src_root_dir/lib/js/src/RepoItem.js src/RepoItem.cmi : build_cmj_cmi src/RepoItem.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/RepoItem.mlast.d src/RepoData.mlast.d src/app_test.mlast.d src/index.mlast.d src/app.mlast.d
